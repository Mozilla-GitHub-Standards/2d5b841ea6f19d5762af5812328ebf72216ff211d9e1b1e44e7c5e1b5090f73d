overlay chrome://browser/content/browser.xul chrome://personas/content/personas.xul

@unpacked@content  personas            chrome/content/
@unpacked@skin     personas class/1.0  chrome/skin/
@unpacked@locale   personas en-US      chrome/locale/en-US/
@unpacked@locale   personas ja         chrome/locale/ja/

@jar@content  personas            jar:chrome/personas.jar!/content/
@jar@skin     personas class/1.0  jar:chrome/personas.jar!/skin/
@jar@locale   personas en-US      jar:chrome/personas.jar!/locale/en-US/
@jar@locale   personas ja         jar:chrome/personas.jar!/locale/ja/
